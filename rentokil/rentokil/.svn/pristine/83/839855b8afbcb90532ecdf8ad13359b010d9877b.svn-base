    <body class="dashboard">


        <div class="container">
            <header class="row-fluid">
                <div class="span6"><h1 class="logo"><a href="#">EDFM</a></h1></div><!-- text is hidden -->
                <div class="span6 alignright userlogininfo">
                    <div class="username">Welcome Guest</div>
                    <br class="clear noSmall" />

                    <div class="accountlinks"><a href="#">Please log in to continue</a></div>
                    <div id="header-divider"><img src="" class="logininfodivider"></div>                    
                </div>
            </header> <!-- /row -->
            <div class="row-fluid" style="margin-top:60px;">
                <div class="row-fluid">
                    <div class="span12">
                        <div class="row-fluid">
                            <div class="span4 widget" style="float:none;margin:0 auto;">
                                <div class="row-fluid">
                                    <div class="span10"><h2>Welcome</h2></div>
                                </div>



                                <div class="row-fluid">
                                    <div class="tab-content">
                                        <div class="span12 aligncenter">
                                            <form name="frmLogin" id="frmLogin" method="post">
                                                <div class="control-group error" >
                                                    <label class="control-label" for="inputError"></label>
                                                    <div class="controls"  >
                                                        <div id="validateerror" class="controls">
                                                            <div id="validateerrorvalue" style="text-align:center; display:none;"><button type='button' class='close' data-dismiss='alert'>&times;</button></div>

                                                        </div> 

                                                        <!-- <p><input type="text" class="alert alert-error" id="inputError" style="text-align:center; display:none;"  disabled><button type="button" class="close" data-dismiss="alert">&times;</button>
                                                        </p> -->
                                                    </div> 
                                                </div>
                                                <h3 class="font">Please log in to continue</h3>
                                                <div>
                                                    <p>     
                                                    <label class="user">Username</label>
                                                    <div id="txtUsernameError">
                                                        <input type="text" placeholder="" id="txtUsername" name="txtUsername">
                                                    </div>
                                                    </p>
                                                </div>
                                                <div>
                                                    <p>     
                                                        <label class="user">Password</label>
                                                    <div id="txtPasswordError">
                                                        <input type="password" placeholder="" id="txtPassword" name="txtPassword">
                                                    </div>
                                                    </p>
                                                </div>
                                                <div class="row-fluid">
                                                    <p>
                                                        <a href="#" role="button" class="btn btn-primary " data-toggle="" id="btnLogin" name="btnLogin">Login</a></p>
                                                        <p><a class="btn btn-small" data-toggle="modal" aria-hidden="true" href="#divForgerPwd">Forgot password </a></p><br>														
														<a href="register.html" >No account yet?  Register here.</a>


                                                    <!--<label class="user" id="InvalidLoginLabel" style="color:red;"></label>-->
                                                </div>
                                            </form>

                                        </div>
                                    </div>
                                </div><!-- /row -->

                            </div>
                        </div><!-- /row -->

                    </div>  
                </div>



            </div> <!-- /row -->
            <div id="divForgerPwd" data-backdrop="static" data-keyboard="false" class="modal hide fade"  data-toggle='modal' tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
              <form name="frmForgotPassword" id="frmForgotPassword" method="post" style="height:196px;">  
				<div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal"aria-hidden="true" id="xNewCustomerAdmin" name="xNewCustomerAdmin">x</button>
                    <h3 id="myModalLabel">Forgot password</h3>
                </div>
                <div class="modal-body">
                   <div class="control-group">
				  
                          <label id="lblUsername" for="Username" class="span1" style="margin-left:0px;">Username</label>                             
                         <input type="text" name="txtforgetUserId" id="txtforgetUserId" value=""/>
						  
                   </div>
                              <label style="margin-right: 15px;"></label>
                               <label>We will send your new password to the email associated with your account.</label>
                                
                </div>
                <div class="modal-footer">                                     
                        <label id="AddPupilFooterLabel" style="margin-left: 0px; float:left;"></label>                   
                    <a class="btn" data-toggle="modal" id="btnNewAddPupilAdminClose" name="btnNewAddPupilAdminClose" data-dismiss="modal" >Close</a>
                    <button class="btn btn-primary" href="#" data-toggle="modal" id="btnNewAddPupilAdminSubmit"
                            name="btnNewAddPupilAdminSubmit">Submit</button>
                </div>
				</form>
            </div> 
			 
			 <div id="divForgerPwdSuccess" data-backdrop="static" data-keyboard="false" class="modal hide fade"  data-toggle='modal' tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true" id="xNewCustomerAdmin" name="xNewCustomerAdmin">x</button>
                    <h3 id="myModalLabel">Forgot password</h3>
                </div>
                <div class="modal-body">
                    <table>
                        <tbody>
                           
                            <tr>
                                <td><label style="margin-right: 15px;"></label>
                                </td>
                                <td><label id="lblsuccesspassword"></label>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
                <div class="modal-footer">
                    <div class="controls" >                       
                    </div> 
                    <div id="AddPupilFooterLabelError">
                        <label id="AddPupilFooterLabel" style="margin-left: 0px; float:left;color:red"></label>						
                    </div>
                    <button class="btn btn-primary" href="#" data-toggle="modal" id="btnForgotLogin"
                            name="btnForgotLogin">Login</button>
                   
                </div>
