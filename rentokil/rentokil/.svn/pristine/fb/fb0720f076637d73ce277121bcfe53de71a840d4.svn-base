.alignright {text-align: right;}
a.small-link {font-weight: normal; color: #878787; text-decoration: underline;font-size: 0.85em;} /* use for small informational links */
.border-top {border-top: 1px solid #cccccc; padding-top: 15px;}
.noBottom, .widget .noBottom {margin-bottom: 0px;}
.top20 {margin-top: 20px;}
.right15 {margin-right: 15px;}
a.moreinfo {display: block; width: 21px; height: 21px; float: right; padding-top: 10px; text-indent: -10000px; line-height: 0px; font-size: 0; background: transparent url('../img/infoicon.png') no-repeat bottom;}
a.centreinfo {padding-top: 0px;float: none;margin: 0 auto;}
div.pseudotable > div {border-top: 1px solid #eeeeee; padding-top: 15px;} /* "pseudo" table with bordered "cells" */
div.pseudotable > div:first-child {border-top: none; padding-top: 0px;}
.center {text-align: center;}
.clear{clear:both;}
/* unfortunate FF-only styles */
@-moz-document url-prefix() {
    span.select-wrap select, span.select-wrap .uneditable-input {top: 3px;}
}

