    <body class="dashboard" load="LoadPageData();">


        <div class="container">
            <header class="row-fluid">
                <div class="span6"><h1 class="logo"><a href="#">EDFM</a></h1></div><!-- text is hidden -->
                <div class="span6 alignright userlogininfo">
                    <div class="username">Welcome Guest</div>
                    <br class="clear noSmall" />

                    <div class="accountlinks"><a href="#">Please register to continue</a></div>
                    <div id="header-divider"><img src="img/eden-header-logininfo-divider.png" class="logininfodivider"></div>                   
                </div>
            </header> <!-- /row -->
            <div class="row-fluid" style="margin-top:60px;width:100%;">
                <div class="row-fluid">
                    <div class="span4 widget" style="float:none;margin:0 auto;">
                        <div class="row-fluid">
                            <div class="span10"><h2>Register</h2></div>
                        </div>
                        <div class="row-fluid">
                            <div class="tab-content">
                                <div class="aligncenter">                                    
                                        <div id="validateerror" class="controls">
                                            <div id="InvalidRegisterLabel" style="text-align:center; display:none;"></div>
                                        </div> 
                                        <h3 class="font">Registration takes seconds</h3>
                                        <br>
                                        <div class="control-group">
                                            <label for="FirstName"  id="lblFirstName" style="text-align:left;display:inline;margin-right:15px;">First Name</label>
                                            <input type="text" id="txtFirstName" name="txtFirstName" maxlength="50" value="" class="text_reg"/>
                                        </div>
                                        <div class="control-group">
                                            <label for="LastName"  id="lblLastName" style="text-align:left;display:inline;margin-right:15px;">Last Name</label>
                                            <input type="text" id="txtLastName" name="txtLastName" maxlength="50" value="" class="text_reg"/>
                                        </div>
                                        <div class="control-group">
                                            <label for="Email"  id="lblEmail" style="text-align:left;display:inline;margin-right:47px;">Email</label>
                                            <input type="text" id="txtEmail" name="txtEmail" maxlength="120" value="" class="text_reg"/>
                                        </div>
                                        <div class="control-group">
                                            <label for="PupilId" id="lblpupilid" style="text-align:left;display:inline;margin-right:33px;">Pupil ID</label>
                                            <input type="text" id="txtPupilId" name="txtPupilId" class="input-xmedium" maxlength="10" value=""/>&nbsp;
                                            <a href="#" id="hoverevent" data-toggle="tooltip" data-placement="left"  data-original-title="Enter a pupil ID to verify your account" ><img src="img/infoicon.png" align="middle" style="vertical-align:text-top;display:inline;margin-top:-5px;"></a>
                                        </div>
                                        <div class="control-group" style="padding-top:10px">
                                            <label id="chkAgree1"><input type="checkbox" value="1" id="chkAgree" name="chkAgree" style="display:inline;margin-top:-2px;"> Yes, I agree with the <a href="interms.html" target="_blank" >T&C's</a></label>
                                            <label style="padding-top:5px"> We'll email you a password</label>
                                        </div>
                                        <br>
                                        <div class="row-fluid" >
                                            <button type="submit" class="btn btn-primary"  data-dismiss="modal" data-toggle="modal" id="btnRegister"
                                                    name="btnRegister" style="margin-top:-15px;margin-bottom:13px;">Register</button>
													<br/>
                                                    <span id="spnGenRegister" class="hide"><img src="img/ajax-loader2.gif">  Registering......</span>
                                                    <br/>
                                            <a href="index.html" >Already have an account? Login here.</a>
                                        </div>
                                </div>
                            </div>
                        </div><!-- /row -->

                        <div id="editBtnSuccessClose" data-backdrop="static" data-keyboard="false" class="modal hide fade" tabindex="-1"
                             role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
                            <div class="modal-header">
                                <button type="button" class="close" data-dismiss="modal"
                                        aria-hidden="true" id="xNewCustomer" name="xNewCustomer">x</button>
                                <h3 id="myModalLabel">Registration success!</h3>
                            </div>
                            <div class="modal-body">
                                <label>Success! We've sent you an email. Check your SPAM folder if you don't receive it</label>
                                <a href="index.html" class="btn btn-primary" style="margin-left: 80%; margin-top: 3%; width: 14%; height: 18px;">Login</a>
                            </div>
                        </div>
                    </div>
