	<script>
	$("#btnLoadForm").click(function(){
		 $("#frmDigitalForm").load("digital_form1.html", function () {			
			 $("#btnLoadData").show();			 
		 });
	});
	$("#btnLoadData").click(function(){
		 LoadData();
	});
	function LoadData(id){
	var submitted_day = (id==0) ? "Thursday" : "Friday";
		
		var xml_string = "<page pageid='formSide1Main' pagenumber='1'><FormCompletedBy>TEST</FormCompletedBy><Qty1>2</Qty1><Qty2>2</Qty2><Qty3>3</Qty3><Qty4>3</Qty4><Qty5>4</Qty5><Qty6>4</Qty6><Qty7>5</Qty7><Qty8>5</Qty8><Qty9>6</Qty9><Qty10>6</Qty10><Qty15>9</Qty15><Qty16>9</Qty16><Qty17>1</Qty17><Qty18>2</Qty18><Qty19>3</Qty19><Qty20>4</Qty20><Qty11>7</Qty11><Qty12>7</Qty12><Qty13>8</Qty13><Qty14>0</Qty14><MenuOption15>A</MenuOption15><MenuOption16>B</MenuOption16><MenuOption17>C</MenuOption17><MenuOption18>D</MenuOption18><MenuOption19>E</MenuOption19><MenuOption20>F</MenuOption20><Ward>Cherry/CCU</Ward><Day>"+submitted_day+"</Day><MenuOption1>Menu Option 1 data...</MenuOption1><MenuOption3 /><MenuOption2 /><MenuOption4 /><MenuOption5 /><MenuOption6 /><MenuOption7 /><MenuOption8 /><MenuOption9 /><MenuOption10 /><MenuOption11 /><MenuOption12 /><MenuOption13 /><MenuOption14 /></page>";
		  $($.parseXML(xml_string)).find( "page" ).children().each(function(){
			  var tagname = this.tagName;
			  var val = $(this).text();
			  if($("#"+tagname).is("label"))
				  $("#"+tagname).html(val);
			  else if($("#"+tagname).is("input"))
				  $("#"+tagname).val(val);
			  else {
				  //For checkbox
				  var ctlName = "#"+tagname+"_"+val.replace(" ","_").replace("/","_");
				  if($(ctlName).is(':checkbox'))
				  	$(ctlName).attr("checked","checked");
			  }
		  });
		  $('input[type=text]',"#frmDigitalForm").attr("disabled","disabled");
		  $('input[type=checkbox]',"#frmDigitalForm").attr("disabled","disabled");
		  $("#btnPrint").show();
	}
	$("#btnLoadFormData").click(function(){
		$("#frmDigitalForm").load("digital_form1.html", function () {			
			LoadData(0);		 
		 });		
	});
	$("#btnLoadFormData1").click(function(){
		$("#frmDigitalForm").load("digital_form1.html", function () {			
			LoadData(1);		 
		 });		
	});
	
$(document).ready(function(){});
