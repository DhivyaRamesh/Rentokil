        <div id="divNewCustomer" class="modal hide fade" tabindex="-1"
             role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
            <form name="frmCreateCustomer" id="frmCreateCustomer" method="post" >
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal"
                            aria-hidden="true" id="xNewCustomer" name="xNewCustomer">x</button>
                    <h3 id="myModalLabel">Create new customer</h3>
                </div>
                <div class="modal-body control-group">
                    <label class="control-label" style="margin-right: 15px;">Customer name &nbsp;&nbsp;<input type="text" name="txtCustomerName" maxlength="50" placeholder="enter text...." id="txtCustomerName" />
                    </label>
                </div>
            </form>
            <div class="modal-footer">
                <label id="Customernameerror" style="margin-left: 0px; float:left;"> </label>
                <button class="btn" data-dismiss="modal" aria-hidden="true">Cancel</button>
                <button class="btn btn-primary" id="btnNewCustomerSubmit"
                        name="btnNewCustomerSubmit">Submit</button>
            </div>
        </div>
        <a class="btn" data-toggle="modal" href="#divSuccess" id="btnSuccess" name="btnSuccess" style="display:none;">HERE</a>
        <div id="divSuccess" class="modal hide fade" tabindex="-1"
             role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal"
                        aria-hidden="true" id="xNewCustomer" name="xNewCustomer">x</button>
                <h3 id="lblSuccessHeading" name="lblSuccessHeading">Success</h3>
            </div>
            <div class="modal-body">
                <label class="radio inline" style="margin-right: 15px;" id="lblSuccessMessage">Success</label>
            </div>
            <div class="modal-footer">
                <button class="btn" data-dismiss="modal" aria-hidden="true" id="btnCloseCust" name="btnCloseCust" >Close</button>
            </div>
        </div>
        <div id="divEditCustomer" class="modal hide fade" tabindex="-1"
             role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal"
                        aria-hidden="true" id="xEditCustomer" name="xEditCustomer">x</button>
                <h3 id="myModalLabel">Edit a Customer</h3>
            </div>
            <div class="modal-body">
                <input type="hidden" name="hdnCustomerId" id="hdnCustomerId" />
                <label class="radio inline" style="margin-right: 15px;">Customer
                    Name: &nbsp;&nbsp;<input type="text" name="txtCustomerName"
